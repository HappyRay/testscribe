format_version: 1
module: test_scribe.create_test_cmd
tests:
- name: test_get_output_root_dir_from_config
  short_name: _from_config
  target:
    name: get_output_root_dir
  parameters:
  - name: output_root_dir
    value: null
  - name: config
    value: !test_scribe/mock 'config'
  result: 1
  mocks:
  - name: config
    spec: test_scribe.config.Config
    attributes:
      output_root_path: !test_scribe/mock 'm_path'
  - name: m_path
    spec: pathlib.Path
    calls:
    - name: resolve
      parameters: []
      return: 1
