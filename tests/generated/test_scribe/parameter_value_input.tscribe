format_version: 1
module: test_scribe.parameter_value_input
tests:
- name: test_get_default_value_from_old_params_no_name_match_index_out_of_bound
  short_name: _no_name_match_index_out_of_bound
  target:
    name: get_default_value_from_old_params
  parameters:
  - name: default
    value: !test_scribe/expression 'test_scribe.namedvalues.NamedValues([(''a'', 1),
      (''b'', 2)])'
  - name: index
    value: 2
  - name: name
    value: c
  result: !test_scribe/callable
    name: NoDefault
    module: test_scribe.special_type
- name: test_get_default_value_from_old_params_no_name_match_use_index
  short_name: _no_name_match_use_index
  target:
    name: get_default_value_from_old_params
  parameters:
  - name: default
    value: !test_scribe/expression 'test_scribe.namedvalues.NamedValues([(''a'',1),
      (''b'',2)])'
  - name: index
    value: 1
  - name: name
    value: c
  result: 2
