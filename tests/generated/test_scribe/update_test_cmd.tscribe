format_version: 1
module: test_scribe.update_test_cmd
tests:
- name: test_create_patcher_from_model_skip
  short_name: _skip
  target:
    name: create_patcher_from_model
  parameters:
  - name: patch_model
    value: !test_scribe/mock 'patch_model'
  - name: test
    value: !test_scribe/mock 'm_test_model'
  result: false
  patches:
  - target: test_scribe.update_test_cmd.prompt
    replacement: !test_scribe/mock 'm_prompt'
  mocks:
  - name: m_prompt
    spec: click.termui.prompt
    calls:
    - parameters:
      - name: text
        value: Create patch with target ( a.b ) replacement ( 1 )?
      - name: default
        value: true
      - name: type
        value: !test_scribe/callable
          name: bool
          module: builtins
      return: false
  - name: patch_model
    spec: test_scribe.model_type.PatchModel
    attributes:
      target: a.b
      replacement: 1
  - name: m_test_model
    spec: test_scribe.model_type.TestModel
- name: test_create_patches_from_existing_test_recreate_all_patches
  short_name: _recreate_all_patches
  target:
    name: create_patches_from_existing_test
  parameters:
  - name: test
    value: !test_scribe/mock 'test'
  result: null
  patches:
  - target: test_scribe.update_test_cmd.create_patcher_from_model
    replacement: !test_scribe/mock 'm_create_patcher_from_model'
  mocks:
  - name: m_create_patcher_from_model
    spec: test_scribe.update_test_cmd.create_patcher_from_model
    calls:
    - parameters:
      - name: patch_model
        value: !test_scribe/mock 'm_patch_model'
      - name: test
        value: !test_scribe/mock 'test'
      return: !test_scribe/expression 'ignore'
    - parameters:
      - name: patch_model
        value: !test_scribe/mock 'm_patch_model_1'
      - name: test
        value: !test_scribe/mock 'test'
      return: !test_scribe/expression 'ignore'
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      patches:
      - !test_scribe/mock 'm_patch_model'
      - !test_scribe/mock 'm_patch_model_1'
  - name: m_patch_model
    spec: test_scribe.model_type.PatchModel
  - name: m_patch_model_1
    spec: test_scribe.model_type.PatchModel
- name: test_create_patches_from_existing_test_no_patch
  short_name: _no_patch
  target:
    name: create_patches_from_existing_test
  parameters:
  - name: test
    value: !test_scribe/mock 'test'
  result: null
  mocks:
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      patches: []
- name: test_get_expression_str_regular_value
  short_name: _regular_value
  target:
    name: get_expression_str
  parameters:
  - name: value
    value: 1
  result: '1'
- name: test_get_expression_str_expression_model
  short_name: _expression_model
  target:
    name: get_expression_str
  parameters:
  - name: value
    value: !test_scribe/expression 'test_scribe.model_type.ExpressionModel("a")'
  result: a
- name: test_infer_output_root_dir_from_module
  short_name: _
  target:
    name: infer_output_root_dir_from_module
  parameters:
  - name: module
    value: !test_scribe/expression 'test_scribe.module.Module(["a", "b", "m"])'
  - name: scribe_file_path
    value: !test_scribe/expression 'pathlib.Path("root/a/b/m.tscribe")'
  result: !test_scribe/object
    type: pathlib.PosixPath
    repr: PosixPath('root')
- name: test_really_create_patcher_from_model_mock
  short_name: _mock
  target:
    name: really_create_patcher_from_model
  parameters:
  - name: patch_model
    value: !test_scribe/mock 'patch_model'
  - name: test
    value: !test_scribe/mock 'm_test_model'
  result: null
  patches:
  - target: test_scribe.update_test_cmd.patch_with_mock_internal
    replacement: !test_scribe/mock 'm_patch_with_mock_internal'
  mocks:
  - name: m_patch_with_mock_internal
    spec: test_scribe.mocking_support.patch_with_mock_internal
    calls:
    - parameters:
      - name: target
        value: a.b
      - name: mock_name
        value: a
      - name: spec
        value: !test_scribe/callable
          name: C
          module: test_data.simple
  - name: patch_model
    spec: test_scribe.model_type.PatchModel
    attributes:
      replacement: !test_scribe/expression 'test_scribe.model_type.MockNameModel("a_2")'
      target: a.b
  - name: m_test_model
    spec: test_scribe.model_type.TestModel
    attributes:
      mocks:
      - !test_scribe/mock 'm_mock_model'
  - name: m_mock_model
    spec: test_scribe.model_type.MockModel
    attributes:
      name: a_2
      spec_str: test_data.simple.C
- name: test_really_create_patcher_from_model_expression
  short_name: _expression
  target:
    name: really_create_patcher_from_model
  parameters:
  - name: patch_model
    value: !test_scribe/mock 'patch_model'
  - name: test
    value: !test_scribe/mock 'm_test_model'
  result: null
  patches:
  - target: test_scribe.update_test_cmd.patch_with_expression_internal
    replacement: !test_scribe/mock 'm_patch_with_expression_internal'
  mocks:
  - name: m_patch_with_expression_internal
    spec: test_scribe.mocking_support.patch_with_expression_internal
    calls:
    - parameters:
      - name: target_str
        value: a.b
      - name: expression
        value: '1'
  - name: patch_model
    spec: test_scribe.model_type.PatchModel
    attributes:
      replacement: 1
      target: a.b
  - name: m_test_model
    spec: test_scribe.model_type.TestModel
- name: test_update_test_cmd_no_such_test
  short_name: _no_such_test
  target:
    name: update_test_cmd
  parameters:
  - name: scribe_file_path
    value: !test_scribe/expression 'pathlib.Path("a/b.tscribe")'
  - name: test_name
    value: t1
  result: 1
  patches:
  - target: test_scribe.update_test_cmd.load_scribe_file
    replacement: !test_scribe/mock 'm_load_scribe_file'
  mocks:
  - name: m_load_scribe_file
    spec: test_scribe.load_scribe_file.load_scribe_file
    calls:
    - parameters:
      - name: scribe_file
        value: !test_scribe/object
          type: pathlib.PosixPath
          repr: PosixPath('a/b.tscribe')
      return: !test_scribe/mock 'm_all_tests'
  - name: m_all_tests
    spec: test_scribe.model_type.AllTests
    calls:
    - name: get_test_index_by_name
      parameters:
      - name: test_name
        value: t1
      return: -1
