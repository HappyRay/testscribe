    value: test_data.service
  result: []
- name: test_is_class_name_function
  short_name: _function
  target:
    name: is_class_name
  parameters:
  - name: module_str
    value: test_data.product
  - name: class_or_function_name
    value: get_a_product_total
  result: false
- name: test_is_class_name_method
  short_name: _method
  target:
    name: is_class_name
  parameters:
