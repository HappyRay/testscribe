  - name: tests
    value:
    - !test_scribe/mock 'm_test_model'
    - !test_scribe/mock 'm_test_model_1'
    - !test_scribe/mock 'm_test_model_2'
    - !test_scribe/mock 'm_test_model_3'
  result: !!python/tuple
  - - !test_scribe/mock 'm_test_model'
    - !test_scribe/mock 'm_test_model_1'
  - - !test_scribe/mock 'm_test_model_2'
    - !test_scribe/mock 'm_test_model_3'
  mocks:
  - name: m_test_model
    spec: test_scribe.model_type.TestModel
    attributes:
      target_func_name: a
  - name: m_test_model_1
    spec: test_scribe.model_type.TestModel
    attributes:
      target_func_name: b
  - name: m_test_model_2
    spec: test_scribe.model_type.TestModel
    attributes:
      target_func_name: f
  - name: m_test_model_3
