format_version: 1
module: test_scribe.move_cmd
tests:
- name: test_add_one_test
  short_name: _
  target:
    name: add_one_test
  parameters:
  - name: all_tests
    value: !test_scribe/mock 'all_tests'
  - name: test
    value: !test_scribe/mock 'test'
  result: !test_scribe/object
    type: test_scribe.model_type.AllTests
    members:
      module: a.b
      tests:
      - !test_scribe/mock 'test'
      - !test_scribe/mock 'm_test_model'
      name_to_index:
        test_f_1: 0
        test_f: 1
  mocks:
  - name: all_tests
    spec: test_scribe.model_type.AllTests
    attributes:
      tests:
      - !test_scribe/mock 'm_test_model'
      module: a.b
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      name: test_f
      short_name: _
      target_func_name: f
      target_class_name: ''
  - name: m_test_model
    spec: test_scribe.model_type.TestModel
    attributes:
      name: test_f
      target_class_name: ''
      target_func_name: f
- name: test_do_move_no_test_to_move
  short_name: _no_test_to_move
  target:
    name: do_move
  parameters:
  - name: module
    value: !test_scribe/expression 'test_scribe.module.Module(["foo", "bar", "m"])'
  - name: output_root_path
    value: !test_scribe/expression 'pathlib.Path("a/b")'
  - name: tests_to_move
    value: []
  result: false
- name: test_does_test_match_target_name_negative
  short_name: _negative
  target:
    name: does_test_match_target_name
  parameters:
  - name: test
    value: !test_scribe/mock 'test'
  - name: class_or_function_name
    value: c
  - name: target_is_class
    value: true
  result: false
  mocks:
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      target_class_name: D
- name: test_does_test_match_target_name_positive
  short_name: _positive
  target:
    name: does_test_match_target_name
  parameters:
  - name: test
    value: !test_scribe/mock 'test'
  - name: class_or_function_name
    value: f
  - name: target_is_class
    value: false
  result: true
  mocks:
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      target_func_name: f
- name: test_get_name_to_compare_with_class
  short_name: _class
  target:
    name: get_name_to_compare_with
  parameters:
  - name: target_is_class
    value: true
  - name: test
    value: !test_scribe/mock 'test'
  result: C
  mocks:
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      target_class_name: C
- name: test_get_name_to_compare_with_func
  short_name: _func
  target:
    name: get_name_to_compare_with
  parameters:
  - name: target_is_class
    value: false
  - name: test
    value: !test_scribe/mock 'test'
  result: f
  mocks:
  - name: test
    spec: test_scribe.model_type.TestModel
    attributes:
      target_func_name: f
- name: test_get_tests_to_move_from_one_file_no_matching_test
  short_name: _no_matching_test
  target:
    name: get_tests_to_move_from_one_file
  parameters:
  - name: class_or_function_name
    value: f
  - name: target_is_class
    value: false
  - name: scribe_file
    value: !test_scribe/expression 'fixture.helper.get_test_result_path()/"service.tscribe"'
  - name: target_module_str
    value: a
  result: []
- name: test_get_tests_to_move_from_one_file_skip
