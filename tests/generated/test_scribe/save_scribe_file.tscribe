format_version: 1
module: test_scribe.save_scribe_file
tests:
- name: test_generate_scribe_file_no_test_calls_remove_file
  short_name: _no_test_calls_remove_file
  target:
    name: generate_scribe_file
  parameters:
  - name: scribe_file_path
    value: !test_scribe/expression 'pathlib.Path("foo")'
  - name: all_tests
    value: !test_scribe/mock 'all_tests'
  result: null
  patches:
  - target: test_scribe.save_scribe_file.remove_file_if_no_test
    replacement: !test_scribe/mock 'm_remove_file_if_no_test'
  mocks:
  - name: m_remove_file_if_no_test
    spec: test_scribe.execution_util.remove_file_if_no_test
    calls:
    - parameters:
      - name: file_path
        value: !test_scribe/object
          type: pathlib.PosixPath
          repr: PosixPath('foo')
      - name: tests
        value: []
      return: true
  - name: all_tests
    spec: test_scribe.model_type.AllTests
    attributes:
      tests: []
- name: test_generate_scribe_file_no_test
  short_name: _no_test
  target:
    name: generate_scribe_file
  parameters:
  - name: scribe_file_path
    value: !test_scribe/expression 'pathlib.Path("foo")'
  - name: all_tests
    value: !test_scribe/mock 'all_tests'
  result: null
  mocks:
  - name: all_tests
    spec: test_scribe.model_type.AllTests
    attributes:
      tests: []
